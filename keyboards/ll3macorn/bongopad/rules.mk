# MCU name
MCU = atmega32u4

# bootloader
BOOTLOADER = atmel-dfu

# Build Options
# comment out to disable the options.
BOOTMAGIC_ENABLE = yes	
MOUSEKEY_ENABLE = no	
EXTRAKEY_ENABLE = yes	
CONSOLE_ENABLE = no	
COMMAND_ENABLE = no    
SLEEP_LED_ENABLE = no  
NKRO_ENABLE = yes
BACKLIGHT_ENABLE = no  
AUDIO_ENABLE = no
ENCODER_ENABLE = yes
OLED_ENABLE = yes
OLED_DRIVER = SSD1306
WPM_ENABLE = yes
RGBLIGHT_ENABLE = no